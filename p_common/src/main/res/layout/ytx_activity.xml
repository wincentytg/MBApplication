<?xml version="1.0" encoding="utf-8"?>
<com.ytg.jzy.p_common.view.CCPLayoutListenerView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ytx_base_view"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/transparent"
    android:fitsSystemWindows="true"
    android:orientation="vertical" >

    <View
        android:id="@+id/view_top_margin"
        android:layout_width="fill_parent"
        android:layout_height="24.0dip"
        android:visibility="gone" />

    <FrameLayout
        android:id="@+id/ytx_content_fl"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" >

        <LinearLayout
            android:id="@+id/ytx_root_view"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical" />

        <RelativeLayout
            android:id="@+id/ytx_float_layer"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:visibility="gone" />

        <Button
            android:id="@+id/ytx_trans_layer"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@color/black"
            android:visibility="gone" />
    </FrameLayout>
</com.ytg.jzy.p_common.view.CCPLayoutListenerView>